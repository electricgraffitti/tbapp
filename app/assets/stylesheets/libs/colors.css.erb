a {color: #5e9408;}
a:hover, .public footer a:hover {color: #82cc0a;}

#quick_launch {background: #676767;}
#quick_launch ul li a:hover {background-color: #363636;}
#quick_launch ul li a.active {background-color: #363636;}
.required_asterisk, abbr[title=required] {color:#e74c3c;}

.form-group.focus .form-control, .form-control:focus {border-color: #82cc0a;}

.public header {color:#5e9408;background:#000;}
.component_header {background:#D4E4B9;}
.btn-default {background: #5e9408; color: #fff;}
.btn-default:hover {background: #82cc0a;}

.public .form-group label, .page_title, .span_link {color:#5e9408;}
.public footer a {color:#535353;}

#left_column_main {border-right: 1px solid #9CB672;}

.empty_message,
.data_bar header {background: #eee;}

.empty_message,
.grid_label {color:#000;}

.page_title {color: #333;}

.component_content.listed_content, .spreadsheet {border-bottom: none;}
 input, textarea {border: 1px solid #BBB}
/*.row {border-top: 1px solid #fff; border-top: 1px solid rgba(255,255,255, 0.9);border-bottom: 1px solid #ddd; border-bottom: 1px solid rgba(0,0,0, 0.1);}*/
.component_left .claim_line,
.row.even, 
.row.even .claim_line, 
.row_item_header, 
.row_header,
.spreadsheet_header,
.expanded_item,
.striped li.listed_single_row:nth-child(even) { background: #ececec; }

.component_table,
.option_list {background: #fff;}

.spreadsheet.no_stripe .row.even {background: none;}

.required_asterisk {color:red;display: inline-block;height: 13px;}
.form_component header {color:#ffa400;}
.component_content.active .header_row, 
.nested_row.active, 
.striped .row.even.active .claim_line, 
.listed_single_row.active, 
.listed_item.row.active {background: #EBF6FE;}

.expanded_item {border-top: 1px solid #0095d8;}
.nested_row {border-top: 1px solid #ccc; border-top: 1px solid rgba(0,0,0,0.2);}
.component_content.active .badge, .nested_row.active .badge {background: #fff; color: #0096d6; border: 1px solid #0096d6; }

.list_summary {background: #0096d6; background: rgb(0, 150, 214);color:#fff;color: rgb(255, 255, 255);border: 1px solid #0096d6; border: 1px solid rgb(0, 150, 214);}
.list_summary ul li.data_point .grid_label {color:#fff;}

.worklist_header li.list_option:hover, 
.toolbar li.list_option:hover {background: #ececec;}

#top_section, 
.component_content,
.full_border .row,
#bottom_section .empty_message, 
.option_list,
.listed_single_row {border: 1px solid #c2c2c2;}
.full_border .row:last-of-type {border: 1px solid #c2c2c2;}

.component_header, 
.spreadsheet_header {border-bottom: 1px solid #c2c2c2;}

.toolbar li {border-left: 1px solid #c2c2c2;}
.expanded_item .row_item:first-of-type {border-left: none;}

.component_right .component_wrap,
.data_bar header,
#left_column_main,
.listed_single_row.row,
.row_item, 
.row_header .row_item_header,
.row_detail .row_item_value  {border-right: 1px solid #c2c2c2;}

#right_column_main .toolbar li:last-of-type {border-right: 1px solid #c2c2c2;}

.line_actions.row_item,
.row_item:last-of-type, 
.row_header .row_item_header:last-of-type, 
.row_detail .row_item_value:last-of-type, 
.last_of_type_horizontal,
ul.listed_single_row_component_content .spreadsheet  li.row:last-of-type { border-right: none;}

.full_border .component_content:hover, 
.full_border .component_content.active,
.component_content:hover, 
.component_content.active, 
.listed_item.row.active,
.listed_single_row:hover,
.listed_single_row.active.row,
.listed_single_row.active.row:hover {border: 1px solid #82CC0A;}
 
.component_content.active.row.even,
.component_content.active.row, 
.listed_item.active.row, .listed_item.active.row.even,
.listed_single_row.active.row, .listed_single_row.active.row.even {background-color: #F4FFE0;}

.row.last {border-bottom: none;} 

.toolbar li.selected, .toolbar li:hover {background-color:#fff;}
.toolbar li.is_disabled:hover {background: none;}

.toggle_select {color:#0096d6;}
.toggle_select.selected {color: #FFA401}



