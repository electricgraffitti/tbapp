html, body, #wrapper, #sidebar, #content {overflow: hidden;}
#content_scroller, #sidebar_scroller, .scroller {overflow-y: scroll;}
#wrapper, header, #msmapp header,
#sidebar, #sidebar_scroller, #sidebar_content,
#content, #content_scroller, #inner_content,
#inner_column, #view_port, #inner_view_port,
#j-main_content,
.viewport,
.full_content, .half_content, .item_properties,
.content_details,
.item,
.cover_art,
.overview_video,
.resource_wrap,
#story_meta, #story_meta .story_actions, #story_meta header, #story_meta article, #story_meta button,
.test_results .content_header,
.test_results .content_header span,
.test_results ul,
.test_results ul li,
.test_results ul li span,
.common_core_standard ul li,
.common_core_standard ul,
.common_core_standard ul li span,
.common_core_standard ul li p,
.column_list .item a,
.column_list .item a img,
.column_list .item .title,
.listed_item, .listed_item a, .listed_item a img, .listed_item a .title,
.test_question,
.question,
.multiple_choice_items,
.multiple_choice_items ul,
.multiple_choice_items ul li,
.test_question_nav,
.content_list_item, .scroll-wrap {position: relative; float: left;box-sizing: border-box;}
.scroll-wrap {padding-bottom: 40px;}
.is_hidden {display:none;}

.margin-top-20 {margin-top: 20px;}

#inner_content a {display: inline-block;}
#content_scroller, .full_section, .item_properties li, .item {position: relative; width: 100%; box-sizing: border-box;}
#msmapp .dark_view .dark_viewport {padding: 10px; background: #5f5f5f;}
#msmapp .dark_view .light_viewport {padding: 10px; background: #fff;}
.full_content {width: 622px; clear: both;}
.full_content.inset_form {width: 602px;}
.full_content.full_inset.inset_form {width: 622px;}
.half_content {width: 50%;}
header {text-shadow: 0 1px 2px rgba(0,0,0,0.3); padding: 10px; font-size: 24px; width: 100%;}
#msmapp header {text-shadow: none;}
#msmapp #content header {padding: 6px 10px; border-bottom: 1px solid #ccc;}
#msmapp.ember-application #content .view_port_header {padding: 6px 10px 5px;}
#msmapp #content header.no_border {border: none;}
#msmapp #content .full_content header {padding: 0px 0px 10px; border: none;}
.inner_content header {padding: 0px;}
.admin .inner_content header {padding: 0px 0px 10px;}
.inner_content p {padding: 20px 0px;}
.inner_content p.cluster_title {margin: 0px; padding: 0px 0px 10px 0px; font-weight: 700;}
header.content_header {padding: 0px 0px 10px;}
#msmapp header {font-size: 13px;}
.header_title {position: relative; float: left; width: 50%; display: block;}
#master_header {border-bottom: 1px solid #888; z-index: 10; padding-right: 20px;}
#content_header, #inner_content_header {border-bottom: 1px solid #ccc; z-index: 5; padding-right: 20px;}
.view_section_header {border-top: 1px solid #ccc;}
#msmapp #content header.focused_header {border: none; padding: 0px 10px 0px 0px;}
#msmapp #content header.focused_header img {border: none; padding: 0px;}
#msmapp #content header.focused_header .header_title {padding: 28px 10px;}

#wrapper {background: #ddd;}
#sidebar {background: #E6EAF0; border-right: 1px solid #B0B0B0; border-top: 1px solid #eee; width: 260px;}
#content {background: #fff; border-top: 1px solid #eee;}
#inner_content, .inner_content {width: 100%;padding: 10px;}
#inner_content.with_columns {padding: 0px;}
#inner_content .view, .full_content {padding: 10px;}

/* Internal */
#column_main ,.column_header, .column_meta, .column_list,
#inner_column {position: relative;float: left;}
#column_main {background: #E7EBF0; border-right: 1px solid #B0B0B0;border-top: 1px solid #eee;}
#inner_column {background: #EFEFEF;border-right: 1px solid #B0B0B0;}
#column_main, #inner_column {width: 200px; box-sizing: content-box;}
.column_meta {width: 200px;}
#column_main .column_header, #inner_column .column_header {font-size: 13px;color: #FFF; width: 100%;background: #6F7986;border-bottom: 1px solid #bdbdbd; border-bottom: 1px solid rgba(0,0,0,0.2);}
#column_main .column_header .title, #inner_column .column_header .title {position: relative;width: 150px;margin: 0px;float: left;padding: 10px;}
#inner_column .column_header .title {padding: 10px 10px 7px;}

#student_story_header {background: #EBEBEB;}
#student_story_header img {width:189px;}
#student_story_header a.active {background: #ccc; color:#444; text-shadow: none;}


#msmapp #column_main .column_header, #msmapp #inner_column .column_header {background: #E7EBF0; font-size: 12px;}
#msmapp .dark_view #inner_column .column_header {background: #616161;font-size: 12px;color: #FFF;}
#msmapp #column_main .column_header .title, #msmapp #inner_column .column_header .title {padding: 7px 10px 6px;color: #6C7785;}
#msmapp .dark_view #inner_column .column_header .title {width: 180px; text-align: center; color:#fff;}
#msmapp #column_main .column_list .item a {padding-left: 35px;}
#msmapp #column_main .column_list .item a.no_icon{padding-left: 10px;}

/*#msmapp #column_main .column_list .item.stories a {padding-left: 15px;}*/
#column_main .column_list .item a, #column_main .column_list .listed_item a, #column_main .content_list_item {border: none;}
.section_toggle, a.section_toggle {padding: 6px 10px;color:#444; background:#f3f3f3; border-bottom: 1px solid #d3d3d3;display:block;}
a.section_toggle:visited, a.section_toggle:active {color:#444;}
.section_toggle:hover, a.section_toggle:hover {background:#a7a7a7;color:#fff;}
.section_toggle.active, a.section_toggle.active {background:#7E7E7E;color:#fff;}
.section_toggle .nav_toggle {margin-right: 5px;}

.column_list {width: 200px; position: relative; background:#E6EAF0;}

.column_list .item a, .column_list .listed_item a, .content_list_item {border-bottom: 1px solid #bdbdbd; border-bottom: 1px solid rgba(0,0,0,0.2); border-top: 1px solid #f9f9f9; border-top: 1px solid rgba(255,255,255,0.3); position: relative; width: 100%; display: block; padding: 7px 10px 5px; box-sizing: border-box;}
.column_list .listed_item a {padding: 5px; height: 55px;}
.column_list .item a img {position: relative;float: left;background: #FFF;border: 1px solid #AAA;}
.column_list .item .title, .listed_item .title  {padding: 5px;}
.listed_item .activity {float: right;display: block;width: 170px;padding: 0px;text-align: right;}
.listed_item .activity.complete {cursor: pointer;}
.listed_item .title {float: left;width: 100%;}
.column_list .full_content {padding: 5px;}
.listed_item {background: #eee; width: 100%;}
.listed_item.inactive, .item.inactive {opacity: 0.5;}
.listed_item a {width: 100%; padding: 5px;}
.listed_item .resource_wrap a {padding: 0px;}
.listed_item img {margin-right: 10px;padding: 5px; border: 1px solid #CCC;}
.listed_item.micro img {padding: 0px; border: 0px;margin: 0px;width: 25px; border: 1px solid #666;}
.listed_item.micro .title {width: 76%;}
.listed_item.micro a.active img {border: 1px solid #ddd;}
.viewport img, .inner_content img {position: relative;float: left;margin: 0 10px 20px 0;border: 1px solid #CCC;padding: 5px;}
#column_main .item:nth-of-type(even), #column_main .item.even, #column_main .listed_item:nth-of-type(2), #column_main .listed_item.even {background: none;}
.item:nth-of-type(even), .item.even, .listed_item:nth-of-type(even), .listed_item.even {background: #fff;}
.item_properties li {padding: 1px;}
.item_properties li .item_title {font-weight: 700; color: #606976;}

.listed_resources ul li {width: auto; background: none; border: none; padding: 0px;}
.listed_resources ul li.listed_item a:hover {background: none;}
.listed_resources ul li img {width: 50px;}
.listed_resources ul li .video img {width: auto; height: 50px;}
.listed_resources ul li a img {width: 62px;}

.content_list_item.listed_item .title {width: 50%;padding: 0px 0px 5px 0px;font-size: 14px;font-weight: 700;}
.content_list_item.listed_item.listed_student .title {width: 400px;padding: 5px;font-size: 12px;font-weight: normal;}
.standard_selector .content_list_item.listed_item .title {width: 76%; padding: 5px; font-size: 12px;font-weight: normal;}
.standard_item p {padding: 0px 30px 0px 0px;}

.content_block header {padding-bottom: 10px;}
.content_details li {padding: 0px 0px 5px; color:#606976;}
.content_details a, .modal_link {color: #0088cc;}
.content_details a:hover, .modal_link:hover {text-decoration: underline;}
.video_thumb {height: 80px;}
.question_text {width: 300px;position: relative;float: left;margin: 0px;padding-right: 20px;padding-bottom: 10px;}
#modal_image {max-width: 800px;max-height:600px;}

#student_story_header.listed_item img {padding: 0px;border: 0px;margin: 0px;width: 40px;border: 1px solid #666;height: 40px;margin: 5px;float: left;}
#student_story_header.listed_item.micro .title {color: #6C7785; width: 137px;}

.cover_art {width: 200px;}
.overview_video {width: 400px;}
.overview p {padding-right: 10px;}

.lesson_response {border-top: 1px solid #ccc;}
.lesson_response .response_header {padding: 0px 0px 7px;}
.lesson_response .left_side {width: 400px; padding-right: 10px; position: relative; float: left;}
.lesson_response .left_side p {margin: 0px;}
.lesson_response .right_side {width: 155px; position: relative; float: right; text-align: right;}
.lesson_response .right_side .resource_wrap {float:right;}
.lesson_response .right_side .resource_wrap .listed_item a {padding: 0px;}
.teacher_view .lesson_response {width: 601px; margin-left: -10px;}

.lesson_response.no-border-top {border-top: none;}

.test_question {background: #F5F5F5;border-top: 1px solid #CCC;}
.multiple_choice_items {background: #F5F5F5;}
.multiple_choice_items ul {width: 100%;}
.multiple_choice_items ul li {width: 100%; padding: 5px 0px 5px 25px;}
.test_question_nav {border-bottom: 1px solid #CCC;border-top: 1px solid #CCC; background: #DDD;color: #777;}
.test_question_nav .nav_buttons {float: right;}
.test_meta {padding: 10px 10px 10px 30px;}
.test_meta li {padding: 3px 0px;}
.test_button {position: absolute;right: 10px;bottom: 10px;}

.test_results .content_header {}
.test_results .content_header span {padding-right: 20px; font-weight: 300; font-size: 12px;}
.test_results ul {position: relative;float: left;padding: 5px 0px;}
.test_results ul li {width: 100%; padding: 3px 0px;}
.test_results ul li span {display: block; padding: 5px;}

.test_results ul {width: 100%;}
.test_results ul li {padding: 1px;}
.test_results ul li.incorrect {padding: 1px;border: 1px solid #F89A9A;}
.test_results ul li.correct {padding: 1px;border: 1px solid #82C979;}
.question_text {width: 100%;box-sizing: border-box;background: #fff; color:#444; padding: 5px;}
.question_text p {margin: 0px; display: inline-block;}
.test_results ul li span p {padding: 0px; margin: 0px; display: inline;}
.test_results ul li span p span {display: inline; padding: 0px; float:none;}

.common_core_standard {width: 600px;}
.common_core_standard ul.standard {width: 580px;}
.common_core_standard ul.sub_standard {width: 560px;}
.common_core_standard ul {padding-left:10px;}
.common_core_standard ul li p {margin: 0px; padding: 0px 0px 10px 0px;width: 500px;}
.common_core_standard ul li span.standard_index_number {display: inline-block; padding-right: 10px;}

#content.single_viewport {overflow-y: auto;}
.viewport .poster_views {padding: 20px; width: 1024px;}
.viewport .poster_view {position: relative; float: left; border: 1px solid #ccc;text-align: center; width: 166px; height: 235px; margin: 0px 20px 20px 0px;padding: 0px;overflow: hidden;}
.viewport .poster_view img {width: 166px; border:none; padding:0px;}
.viewport .poster_view .counter_icon {}
.viewport .poster_view .title {position: relative;float: left;display: block;padding: 5px 10px 5px;color: #FFF;background: #888;width: 146px;text-shadow: 0px 1px 0px #000;}

.public_story.full_content {width: 1024px; padding-bottom: 30px;}
.public_story.full_content .left_side {width: 640px;}
.public_story.full_content .right_side {width: 360px;}
.public_story.full_content .right_side ul {padding: 10px;}
.public_story.full_content .right_side .column_list {width: 360px; background: none;}
.public_story.full_content #story_meta {border: 1px solid #ccc;}
.public_story.full_content #story_meta .story_actions {border-bottom: 1px solid #eee; padding: 10px;}
.public_story.full_content #story_meta header {font-size: 24px; width: 485px; overflow: hidden; padding: 0px;}
.public_story.full_content #story_meta article {padding: 10px;}

#msmapp #content .public_story.full_content .spacer_header {padding: 10px;color: #333;background-color: #EEE;border: 1px solid #CCC;}
.listed_story_assignment .full_content {width: 618px;}
.listed_story_assignment { padding: 10px; border: 1px solid #ccc; border-top: 1px solid #eee;}
.listed_story_assignment .lesson_response {margin-top: 10px; border-top: 1px solid #ccc;}
.listed_story_assignment .lesson_response .response_header {padding: 0px 0px 7px;}
.listed_story_assignment .lesson_response .left_side {width: 400px;}
.listed_story_assignment .lesson_response .left_side p {margin: 0px;}
.listed_story_assignment .lesson_response .right_side {width: 155px;}
.listed_story_assignment .lesson_response .right_side .resource_wrap {float:right;}
.listed_story_assignment .lesson_response .right_side .resource_wrap .listed_item a {padding: 0px;}

.admin .listed_trainer_answer p {padding: 0px;}
.admin .listed_trainer_answer {border-bottom: 1px solid #DDD;}

#inner_column .two_icon_header .title {width: 120px;}


